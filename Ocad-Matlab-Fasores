POLAR clear all; clc; A = [5+7j -2j -5 0; -2j 6-2j 0 0; -5+4j -4j 1 0; -4j 4j 0 10]; ... %matriz de coeficientes b = [5-8.7j;3.5-2j;0;0+2j]; %términos independientes I = inv(A)*b %solución del sistema (corrientes de malla) for i = 1:4 disp('I'); disp(i); disp(' = '); disp(abs(I(i))); disp('ang = '); disp(angle(I(i))*180/pi);
end


